    "version": "0.2.0",
    "configurations": [    

        {
            "name": "iOS Dev",
            "request": "launch",
            "type": "dart",
            "args": [
                "--no-sound-null-safety"
            ]
        },
        {
            "name": "iOS QA",
            "request": "launch",
            "type": "dart",
            "args": [
                "--no-sound-null-safety",
                "--dart-define=ENV_FILE=assets/env/qa.env"
            ]
        },
        {
            "name": "Web Chrome - QA",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome",
                "--no-sound-null-safety",
                "--dart-define=ENV_FILE=assets/env/qa.env"
            ]
        },
        {
            "name": "Web Chrome - Dev",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome",
                "--no-sound-null-safety"
            ]
        },
        {
            "name": "Web Edge - QA",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "edge",
                "--no-sound-null-safety",
                "--dart-define=ENV_FILE=assets/env/qa.env"
            ]
        },
        {
            "name": "Web Edge - Dev",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "edge",
                "--no-sound-null-safety"
            ]
        }
    ]
}